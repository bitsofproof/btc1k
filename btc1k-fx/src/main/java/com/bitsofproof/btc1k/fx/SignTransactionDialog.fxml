      <children>
        <Label fx:id="messageLabel" text="Passphrase" textAlignment="LEFT" wrapText="true">
          <font>
            <Font name="System Bold" size="13.0" />
          </font>
        </Label>
        <PasswordField fx:id="passphrase" prefWidth="200.0" promptText="Passhprase" />
