<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<fx:root type="javafx.scene.layout.HBox" prefHeight="-1.0" prefWidth="-1.0" spacing="5.0" xmlns:fx="http://javafx.com/fxml/1"
         xmlns="http://javafx.com/javafx/2.2">
	<Button fx:id="signButton" mnemonicParsing="false" text="Sign"/>
	<GridPane>
		<Label text="Amount" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
		<Label text="Created at" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
		<Label text="Address" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
		<Label fx:id="amountLabel" text="" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
		<Label fx:id="addressLabel" text="" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
		<Label fx:id="createdAtLabel" text="" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
		<columnConstraints>
			<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
			<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
		</columnConstraints>
		<rowConstraints>
			<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
			<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
			<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
		</rowConstraints>
	</GridPane>
	<padding>
		<Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
	</padding>
</fx:root>
